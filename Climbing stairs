#Climbing stairs

class Solution {
public:
    int climbStairs(int n) {
        // return f(0,n);
        vector<int> dp(n + 1, 0);
        for (int i = n; i >= 0; i--) {
            if (i == n)
                dp[i] = 1;
            else {
                int x = (i + 1) <= n ? dp[i + 1] : 0;
                int y = (i + 2) <= n ? dp[i + 2] : 0;
                dp[i] = x + y;
            }
        }
        return dp[0];
    }
};
