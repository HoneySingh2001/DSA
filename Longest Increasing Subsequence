# Longest Increasing Subsequence

class Solution {
public:
    int lengthOfLIS(vector<int>& a) {
        int n=a.size();
        int dp[n];
        int maxi=1;
        dp[0]=1;
        for(int i=1;i<n;i++){
            dp[i]=1;
            for(int j=0;j<i;j++){
                if(a[j]<a[i]){
                    dp[i]=max(dp[i],1+dp[j]);
                }
            }
            maxi=max(maxi,dp[i]);
        }
        return maxi;
    }
};
