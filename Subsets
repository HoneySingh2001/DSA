#  Subsets


void f(vector<vector<int>> &ans,vector<int> v,vector<int>& a,int n,int i){
    if(i==n){
        ans.push_back(v);
        return;
    }
    v.push_back(a[i]);
    f(ans,v,a,n,i+1);
    v.pop_back();
    f(ans,v,a,n,i+1);
}
    vector<vector<int>> subsets(vector<int>& nums) {
        int n=nums.size();
        vector<vector<int>> ans;
        vector<int> v;
        f(ans,v,nums,n,0);
        return ans;
    }
